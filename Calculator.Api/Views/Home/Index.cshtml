@section body{
    <table>
        <thead><tr>
            <th>Passenger name</th><th>Meal</th><th>Surcharge</th><th></th>
        </tr></thead>
        <tbody data-bind="foreach: items">
        <tr>
            <td data-bind="text: val"></td>
        </tr>    
        </tbody>
    </table>
    <p>Input: <input data-bind="value: input, event:{keypress: keypressed}" /><button data-bind="click: calculate">Calculate</button></p>
    @*<p>Output: <strong data-bind="text: output"></strong></p>*@
}